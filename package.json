{
  "name": "wireframe",
  "version": "2.0.0-alpha.1",
  "description": "High-performance, vendor-agnostic AI Assistant Ecosystem",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/connectors/*",
    "packages/plugins/*",
    "packages/templates/*"
  ],
  "type": "module",
  "main": "./src/index.ts",
  "sideEffects": false,
  "bin": {
    "wireframe": "./packages/cli/dist/index.js"
  },
  "scripts": {
    "dev": "wrangler dev --env development --var ENVIRONMENT:development",
    "dev:hot": "node scripts/dev-hot-reload.js",
    "dev:remote": "wrangler dev --env development --var ENVIRONMENT:development --remote",
    "build": "node esbuild.config.js",
    "build:analyze": "node scripts/analyze-bundle.js",
    "deploy": "wrangler deploy --env production",
    "deploy:staging": "wrangler deploy --env staging",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md,yml,yaml}\"",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint && npm run format:check",
    "setup:check": "node scripts/check-setup.js",
    "setup:bot": "node scripts/setup-bot.js",
    "setup:wizard": "node scripts/setup-bot-enhanced.js",
    "setup:docs": "node scripts/generate-project-docs.js",
    "prepare": "husky || true",
    "db:generate": "wrangler d1 migrations create",
    "db:apply:local": "wrangler d1 migrations apply DB --local",
    "db:apply:remote": "wrangler d1 migrations apply DB --env production",
    "db:types": "tsx src/cli/generate-db-types-standalone.ts",
    "db:types:watch": "tsx src/cli/generate-db-types.ts --watch",
    "secrets:list": "wrangler secret list --env production",
    "tail": "wrangler tail --env production",
    "contribute": "tsx src/cli/contribute.ts",
    "docs:generate": "node scripts/generate-claude-setup.js",
    "docs:check": "node scripts/generate-claude-setup.js --check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/talkstream/typescript-wireframe-platform.git"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.61",
    "@cloudflare/workers-types": "^4.20250807.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "chalk": "^5.3.0",
    "chokidar": "^4.0.3",
    "commander": "^14.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^16.3.0",
    "handlebars": "^4.7.8",
    "husky": "^9.1.7",
    "inquirer": "^12.8.2",
    "lint-staged": "^16.1.0",
    "miniflare": "^4.20250803.0",
    "ora": "^8.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.28.1"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.70",
    "@google/genai": "^1.13.0",
    "@sentry/cloudflare": "^10.2.0",
    "dayjs": "^1.11.13",
    "grammy": "^1.37.0",
    "hono": "^4.8.12",
    "zod": "^4.0.15"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "telegram",
    "bot",
    "cloudflare",
    "workers",
    "typescript",
    "grammy",
    "wireframe",
    "template",
    "cli",
    "create-app"
  ],
  "author": "Arseniy Kamyshev",
  "license": "MIT"
}
